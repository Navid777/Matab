{
    "success": true,
    "count": {{ therapists.count }},
    "patients": [
        {% for t in therapists %}
            {
                "first_name": "{{ t.first_name }}",
                "last_name": "{{ t.last_name }}",
                "medical_number": "{{ t.medical_number }}"
            }{% if not forloop.last %},{% endif %}
        {% endfor %}
    ]
}