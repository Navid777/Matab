{
    "success": true,
    "count": {{ patient_turns.count }},
    "patient_turns": [
        {% for p in patient_turns %}
            {
                "id": "{{ p.id }}",
                "first_name": "{{ p.patient.first_name }}",
                "last_name": "{{ p.patient.last_name }}",
                "national_code": "{{ p.patient.national_code }}"
            }{% if not forloop.last %},{% endif %}
        {% endfor %}
    ]
}